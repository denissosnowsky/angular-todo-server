  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:coverageWithHtml": "jest --coverage && start ./coverage/lcov-report/index.html",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "npx lint-staged --relative",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "prettier:staged": "pretty-quick --pattern \"src/**/*.ts\" --staged",
    "pre-commit-check": "npm run prettier:staged && npm run lint:staged"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": "npm run lint:fix"
  },
